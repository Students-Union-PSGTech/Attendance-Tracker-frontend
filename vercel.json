{
  "version": 2,
  "builds": [
    {
      "src": "attendance-trackee/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "attendance-trackee/dist",
        "buildCommand": "npm run vercel-build"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/attendance-trackee/dist/index.html" }
  ]
}
